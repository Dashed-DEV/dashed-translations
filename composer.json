{
    "name": "qubiqx/qcommerce-translations",
    "description": "This is my package qcommerce-translations",
    "keywords": [
        "Qubiqx",
        "laravel",
        "qcommerce-translations"
    ],
    "homepage": "https://github.com/qubiqx/qcommerce-translations",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@qubiqx.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.15"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Qubiqx\\QcommerceTranslations\\": "src",
            "Qubiqx\\QcommerceTranslations\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qubiqx\\QcommerceTranslations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qubiqx\\QcommerceTranslations\\QcommerceTranslationsServiceProvider"
            ],
            "aliases": {
                "QcommerceTranslations": "Qubiqx\\QcommerceTranslations\\Facades\\QcommerceTranslations",
                "Translation": "Qubiqx\\QcommerceTranslations\\Models\\Translation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
